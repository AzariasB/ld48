; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[  ]
_global_script_class_icons={
}

[application]

config/name="Ld48"
config/description="Ld48"
run/main_scene="res://scenes/menu.tscn"
run/disable_stdout=true
run/disable_stderr=true
config/icon="res://icon.png"
config/macos_native_icon="res://icon.png"
config/windows_native_icon="res://icon.png"

[autoload]

global="*res://scripts/global.gd"
Transition="*res://scenes/Transition.tscn"

[display]

window/size/height=576
window/size/test_width=1024
window/size/test_height=576
window/dpi/allow_hidpi=true
mouse_cursor/custom_image="res://assets/textures/cursor.png"
mouse_cursor/custom_image_hotspot=Vector2( 63, 70 )
window/stretch/mode="2d"
window/stretch/aspect="expand"

[logging]

file_logging/enable_file_logging.pc=false

[rendering]

quality/intended_usage/framebuffer_allocation=0
quality/intended_usage/framebuffer_allocation.mobile=1
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_clear_color=Color( 0.133333, 0.133333, 0.133333, 1 )
